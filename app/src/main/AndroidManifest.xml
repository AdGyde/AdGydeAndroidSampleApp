<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.adgyde.myapplication">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:name="com.adgyde.example.ExampleApplication">

        <activity android:name="com.adgyde.example.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.adgyde.example.UserProfile"/>

       <activity android:name="com.adgyde.example.ComputingEvent"/>

        <activity android:name="com.adgyde.example.CountingEvent"/>

        <!--If you are not using -->
        <service
            android:name="com.adgyde.android.FIIDService">
            <intent-filter>
                <action android:name="com.google.firebase.INSTANCE_ID_EVENT"/>
            </intent-filter>
        </service>
        <!--
         If application uses multiple INSTALL_REFERRER receivers, you should use
         com.adgyde.android.MultiInstallReceiver to replace com.adgyde.android.InstallReceiver.
         MultiInstallReceiver MUST be the first receiver before other receivers listening
         INSTALL_REFERRER.
       -->
        <!--
        REMOVE COMMENT TO USE MultiInstallReceiver. Remember to remove InstallReceiver.
        <receiver android:name="com.adgyde.android.MultiInstallReceiver" android:exported="true">
            <intent-filter>
                <action android:name="com.android.vending.INSTALL_REFERRER" />
            </intent-filter>
        </receiver>
        -->
        <receiver
            android:name="com.adgyde.android.InstallReceiver"
            android:exported="true">
            <intent-filter>
                <action android:name="com.android.vending.INSTALL_REFERRER" />
            </intent-filter>
        </receiver>

        <service
            android:name="com.adgyde.android.AppJobService"
            android:exported="true"
            android:permission="android.permission.BIND_JOB_SERVICE">
        </service>

    </application>

</manifest>